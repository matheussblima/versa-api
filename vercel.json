{
  "version": 2,
  "buildCommand": "yarn build:vercel",
  "builds": [
    {
      "src": "dist/src/main.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "certs/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/main.js"
    }
  ],
  "functions": {
    "dist/src/main.js": {
      "maxDuration": 30
    }
  }
}
